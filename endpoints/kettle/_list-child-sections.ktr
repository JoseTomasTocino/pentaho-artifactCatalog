    <sql>SELECT s.id, s.title, s.visible, s.image_url, s.html_description, nc.n_children, nr.n_resources&#xa;FROM sections s&#xa;-- Administrators get child section &#x2b; resource counts &#x28;for the deletion prompt&#x29;&#xa;LEFT JOIN &#x28;SELECT parent, COUNT&#x28;1&#x29; n_children&#xa;           FROM sections&#xa;           GROUP BY parent&#xa;           HAVING parent IS NOT NULL AND &#x3f; &#x7e; &#x27;&#x28;&#x5e;&#x7c;,&#x29;Administrator&#x28;&#x24;&#x7c;,&#x29;&#x27;&#x29; nc ON s.id &#x3d; nc.parent&#xa;LEFT JOIN &#x28;SELECT section_id, COUNT&#x28;1&#x29; n_resources&#xa;           FROM sections_resources&#xa;           GROUP BY section_id&#xa;           HAVING &#x3f; &#x7e; &#x27;&#x28;&#x5e;&#x7c;,&#x29;Administrator&#x28;&#x24;&#x7c;,&#x29;&#x27;&#x29; nr ON s.id &#x3d; nr.section_id&#xa;WHERE s.parent &#x3d; &#x3f; AND &#x28; &#x28; s.visible AND s.allowed_roles &#x26;&#x26; &#x3f;&#x3a;&#x3a;character varying&#x5b;&#x5d; &#x29; OR &#x3f; &#x7e;&#x2a; &#x27;&#x28;&#x5e;&#x7c;,&#x29;Administrator&#x28;&#x24;&#x7c;,&#x29;&#x27; &#x29;&#xa;ORDER BY s.title&#xa;</sql>
    <limit>0</limit>
